{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "src",
    "baseUrl": "./src",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@auth/*": ["./auth/*"],
      "@note/*": ["./note/*"]
    },
    "outDir": "dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
